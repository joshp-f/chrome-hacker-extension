{
  "manifest_version": 2,

  "name": "Traversy Launcher",
  "description": "Quick launch Traversy Media",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
  { "run_at" :"document_end",
  "matches": ["<all_urls>"],
  "html":["index.html"]
  }
  ],
  "permissions": ["activeTab"],
  "web_accessible_resources": ["*"]

}
